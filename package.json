{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "build:gh-page": "bash deploy.sh",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@antv/g6": "^3.8.1",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.23",
    "antd": "^4.7.0",
    "jsmind": "^0.4.6",
    "lint-staged": "^10.0.7",
    "mind-elixir": "^0.11.6",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.23",
    "yorkie": "^2.0.0"
  }
}
